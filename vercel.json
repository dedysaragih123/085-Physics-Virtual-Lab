{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "Server/index.js",
        "use": "@vercel/node",
        "config":{
          "buildCommand" : "cd ./server && npm install & npm run nyala"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  